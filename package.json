      "pre-commit": "lint-staged",
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS"
    }
  },
  "lint-staged": {
    "src/**/*.{js,jsx,ts,tsx,vue}": [
      "npm run lint"
    ],
    "src/**/*.{vue,css,less,sass,scss}": [
      "npm run stylelint"
    ]
